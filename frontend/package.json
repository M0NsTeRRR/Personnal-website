{
  "name": "frontend",
  "version": "0.1.0",
  "description": "Frontend for DRF",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.1.3",
    "axios": "^0.21.0",
    "sass-loader": "^12.6.0",
    "vue": "^2.6.6",
    "vue-recaptcha": "^1.2.0",
    "vue-router": "^3.0.5",
    "vuetify": "^2.0.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.3",
    "@vue/cli-plugin-eslint": "^4.0.3",
    "@vue/cli-service": "^4.0.3",
    "babel-eslint": "^10.0.1",
    "eslint": "^7.7.0",
    "eslint-plugin-vue": "^7.5.0",
    "sass": "^1.22.7",
    "stylus": "^0.55.0",
    "stylus-loader": "^3.0.1",
    "vue-cli-plugin-vuetify": "^2.0.2",
    "vue-template-compiler": "^2.5.21",
    "vuetify-loader": "^1.2.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
